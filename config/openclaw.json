{
  "commands": {
    "native": "auto",
    "nativeSkills": "auto",
    "restart": true
  },
  "channels": {
    "slack": {
      "mode": "socket",
      "webhookPath": "/slack/events",
      "enabled": true,
      "userTokenReadOnly": true,
      "groupPolicy": "open",
      "dm": {
        "enabled": true,
        "policy": "pairing"
      }
    }
  },
  "gateway": {
    "mode": "local"
  },
  "plugins": {
    "entries": {
      "slack": {
        "enabled": true
      }
    }
  }
}
